			"type": "string",
			"defaultValue": "https://synapseworkshopstorage.dfs.core.windows.net"
		},
		"ADF-SSIS-Runtime_properties_typeProperties_ssisProperties_catalogInfo_catalogServerEndpoint": {
			"type": "string",
			"defaultValue": "01sql5vumf.database.windows.net"
		},
		"ADF-SSIS-Runtime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminUserName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"ADF-SSIS-Runtime_properties_typeProperties_ssisProperties_catalogInfo_catalogAdminPassword": {
			"type": "secureString"
		}
	},
